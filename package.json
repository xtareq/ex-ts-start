    "@types/node": "^16.10.3",
    "@types/nodemailer": "^6.4.4",
    "@types/sharp": "^0.29.3",
    "@types/shelljs": "^0.8.9",
    "nodemon": "^2.0.13",
    "npm-run-all": "^4.1.5",
    "shelljs": "^0.8.4",
