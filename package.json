{
  "name": "ex-ts-start",
  "version": "1.2.0",
  "description": "A express-typescript-mysql starter app with basic authentication.",
  "license": "MIT",
  "bin": "./bin/cli.js",
  "scripts": {
    "compile": "tsc -p .",
    "compile:watch": "tsc -w",
    "clean": "rm -rf dist",
    "start": "node ./dist/server.js",
    "copyPublic": "cp -r public ./dist",
    "serve": "npm-run-all compile copyPublic start",
    "dev": "nodemon ./src/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xtareq/ex-ts-start"
  },
  "keywords": [
    "express",
    "typescript",
    "mariadb",
    "mysql",
    "sequelize",
    "REST",
    "authentication"
  ],
  "author": {
    "name": "Tareq Hossain",
    "email": "xtrinsic96@gmail.com",
    "url": "https://www.linkedin.com/in/xtrinsictareq/"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.5.0",
    "@types/jsonwebtoken": "^8.5.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "ex-helpers": "^1.0.5",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-jwt": "^6.1.0",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^2.5.4",
    "nodemailer": "^6.7.0",
